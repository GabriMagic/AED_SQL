<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane fx:id="pCEView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <top>
      <HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <Label text="ISBN:" />
            <ComboBox fx:id="pCECombo" />
            <Button fx:id="pCEMostrar" mnemonicParsing="false" text="Mostrar" />
         </children>
      </HBox>
   </top>
   <center>
      <Label fx:id="pCELabel" BorderPane.alignment="CENTER">
         <padding>
            <Insets top="10.0" />
         </padding>
      </Label>
   </center>
</BorderPane>
